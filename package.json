    "moleculer-db-adapter-mongoose": "^0.8.7",
    "moleculer-web": "^0.9.0",
    "mongoose": "^5.9.29",
    "nats": "^1.3.0"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
